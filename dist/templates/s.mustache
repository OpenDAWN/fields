<!doctype html>

<html>
  
  <head>
    <link href="css/sound.css" rel="stylesheet" type="text/css" />
    <script src="js/sound.js"></script>
    <script src="js/config.js"></script>
    {{#extraInstruments}}
    <script src="{{{.}}}"></script>
    {{/extraInstruments}}
  </head>

  <body>
    <div id="mapContainer">
      <div id="map" style="color:white; background-color:black; margin:auto; margin-top:1em;padding-top:1em;padding-bottom: 1em; border: solid 1px white">PRESS TO START</div>
      <!--<img id="map" src="images/map.svg" />-->
      <!--<h2>The drawing above is a map of the room you are sitting in. Please locate yourself by touching the map.</h2>-->
    </div>
    <div id="console"></div>
    <div id="status"></div>

    <script>
      if (!fields.isSupported()) {
        $('body').html('<div id="notSupported">Sorry, but this web browser is not supported by our system. You should try with a recent version of Chrome or Firefox.</div>')
      } else {
        fields.log('Device is supported')
      }
    </script>

  </body>
  
</html>
